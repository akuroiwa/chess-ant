"P(s_{L} , a) = p_{a}"

#: ../../ant_baits.rst:49
msgid "Update:"
msgstr "更新："

#: ../../ant_baits.rst:51
msgid ""
"t \\leq L\n"
"\n"
"N(s_{t} , a_{t}) = N(s_{t} , a_{t}) + 1\n"
"\n"
"W (s_{t} , a_{t}) = W(s_{t} , a_{t}) + v\n"
"\n"
"Q(s_{t} , a_{t}) = \\frac{W(s_{t} , a_{t})}{N(s_{t} , a_{t})}"
msgstr ""
"t \\leq L\n"
"\n"
"N(s_{t} , a_{t}) = N(s_{t} , a_{t}) + 1\n"
"\n"
"W (s_{t} , a_{t}) = W(s_{t} , a_{t}) + v\n"
"\n"
"Q(s_{t} , a_{t}) = \\frac{W(s_{t} , a_{t})}{N(s_{t} , a_{t})}"

